-- 1.5 特殊动态文本规则（固定格式，用正则）
local specialDynamicText = {
    {pattern = "Return Delay (sec): %d+%.?%d*", replace = "返回延迟（秒）：%1"},
    {pattern = "Activation Time (s): %d+%.?%d*", replace = "激活时间（秒）：%1"},
}

return {
    staticText = staticText,
    btnText = btnText,
    espText = espText,
    dynamicPrefixText = dynamicPrefixText,
    specialDynamicText = specialDynamicText
}
]]

-- ================ 3. 核心工具函数（无需修改） ================
-- 获取最终保存路径（跨注入器/跨系统：Windows/macOS自动适配）
local function getFinalSavePath()
    local basePath
    -- 优先使用注入器提供的沙盒路径
    if getPathFunc then
        basePath = getPathFunc()
    else
        -- 降级路径（适配无沙盒的注入器）
        basePath = "C:/RobloxCustomFiles" -- Windows默认
        pcall(function()
            -- 自动检测macOS系统并切换路径
            if string.find(game:GetService("MarketplaceService"):GetProductInfo(1).Name, "Mac") then
                basePath = "~/Library/RobloxCustomFiles" -- macOS默认
            end
        end)
    end
    -- 拼接完整路径（确保路径合法，去除特殊字符）
    local safeFileName = CUSTOM_FILE_NAME:gsub("[\\/:*?\"<>|]", "_") -- 替换非法字符为下划线
    return basePath .. "/" .. safeFileName
end

-- 静默保存文件（无UI，仅控制台输出结果）
local function silentSaveFile()
    -- 检查注入器是否支持文件写入
    if not writeFunc then
        warn("当前注入器不支持文件写入功能")
        return
    end

    local savePath = getFinalSavePath()
    -- 执行保存（内容原样写入，不做任何格式修改）
    local success, err = pcall(writeFunc, savePath, CUSTOM_FILE_CONTENT)
    
    if success then
        warn("配置文件保存成功: " .. savePath)
    else
        warn("配置文件保存失败: " .. tostring(err))
    end
end

-- ================ 4. 启动入口（无需修改） ================
pcall(function()
    -- 脚本执行后立即静默保存
    silentSaveFile()
end)
