setreadonly(m, false)

-- 优化hook函数，使用缓存提高性能
m.__newindex = newcclosure(function(t, k, v)
    if k == "Text" then
        local objectType = typeof(t)
        if objectType == "Instance" and (t:IsA(TextLabel) or t:IsA(TextButton) or t:IsA(TextBox)) then
            v = V[v] or v
        end
    end
    return o(t, k, v)
end)

setreadonly(m, true)
