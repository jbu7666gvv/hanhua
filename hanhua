-- è·¨æœè‡ªåŠ¨åŠ è½½å™¨ï¼ˆæ”¾åœ¨æœ€å¼€å¤´ï¼‰
do
    -- å¢å¼ºç‰ˆè·¨æœåŠ è½½å™¨
    local crossServerKey = "BHBUO_CrossServer_v2"
    if _G[crossServerKey] then return end
    _G[crossServerKey] = true
    
    -- ä¿å­˜åŸå§‹è·¨æœå‡½æ•°
    local originalQueueTeleport = (syn and syn.queue_on_teleport) or queue_on_teleport or (fluxus and fluxus.queue_on_teleport)
    
    if originalQueueTeleport then
        -- è·¨æœåé‡æ–°åŠ è½½æœ¬è„šæœ¬
        local teleportCode = [[
            -- è·¨æœåé‡æ–°åŠ è½½è„šæœ¬
            wait(1)
            loadstring(game:HttpGet("https://raw.githubusercontent.com/jbu7666gvv/hanhua/refs/heads/main/hanhua"))()
        ]]
        
        -- è®¾ç½®è·¨æœä»£ç 
        originalQueueTeleport(teleportCode)
        
        -- åŒæ—¶è®¾ç½®äº‹ä»¶ç›‘å¬ï¼ˆåŒé‡ä¿é™©ï¼‰
        if game.Players.LocalPlayer then
            game.Players.LocalPlayer.OnTeleport:Connect(function(state)
                if state == Enum.TeleportState.Started then
                    originalQueueTeleport(teleportCode)
                end
            end)
        end
    end
end

-- ============================ å®éªŒè„šæœ¬ ============================

-- åˆ›å»ºç®€å•çš„UIæ˜¾ç¤º"ä½ å¥½"
local function createHelloUI()
    -- åˆ›å»ºScreenGui
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "HelloGUI"
    screenGui.Parent = game:GetService("CoreGui")
    
    -- åˆ›å»ºä¸»æ¡†æ¶
    local mainFrame = Instance.new("Frame")
    mainFrame.Size = UDim2.new(0, 300, 0, 200)
    mainFrame.Position = UDim2.new(0.5, -150, 0.5, -100)
    mainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
    mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    mainFrame.BackgroundTransparency = 0.1
    mainFrame.BorderSizePixel = 2
    mainFrame.BorderColor3 = Color3.fromRGB(0, 170, 255)
    mainFrame.Active = true
    mainFrame.Draggable = true
    mainFrame.Parent = screenGui
    
    -- åœ†è§’
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 10)
    corner.Parent = mainFrame
    
    -- æ ‡é¢˜æ 
    local titleBar = Instance.new("Frame")
    titleBar.Size = UDim2.new(1, 0, 0, 30)
    titleBar.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
    titleBar.BorderSizePixel = 0
    titleBar.Parent = mainFrame
    
    local titleCorner = Instance.new("UICorner")
    titleCorner.CornerRadius = UDim.new(0, 10)
    titleCorner.Parent = titleBar
    
    -- æ ‡é¢˜æ–‡æœ¬
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(1, 0, 1, 0)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Text = "å®éªŒè„šæœ¬"
    titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextSize = 18
    titleLabel.Parent = titleBar
    
    -- ä¸»å†…å®¹ - æ˜¾ç¤º"ä½ å¥½"
    local helloLabel = Instance.new("TextLabel")
    helloLabel.Size = UDim2.new(1, 0, 1, -30)
    helloLabel.Position = UDim2.new(0, 0, 0, 30)
    helloLabel.BackgroundTransparency = 1
    helloLabel.Text = "ğŸ‘‹ ä½ å¥½ï¼"
    helloLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    helloLabel.Font = Enum.Font.GothamBold
    helloLabel.TextSize = 50
    helloLabel.Parent = mainFrame
    
    -- åº•éƒ¨ä¿¡æ¯
    local infoLabel = Instance.new("TextLabel")
    infoLabel.Size = UDim2.new(1, 0, 0, 20)
    infoLabel.Position = UDim2.new(0, 0, 1, -20)
    infoLabel.BackgroundTransparency = 1
    infoLabel.Text = "è·¨æœæµ‹è¯•è„šæœ¬ | é‡è¿›åè‡ªåŠ¨æ˜¾ç¤º"
    infoLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
    infoLabel.Font = Enum.Font.Gotham
    infoLabel.TextSize = 12
    infoLabel.Parent = mainFrame
    
    -- æ˜¾ç¤ºæç¤º
    print("å®éªŒè„šæœ¬å·²å¯åŠ¨ - æ˜¾ç¤º'ä½ å¥½'")
    print("è·¨æœä»£ç å·²åŠ è½½ - é‡è¿›åä¼šè‡ªåŠ¨é‡æ–°è¿è¡Œ")
    
    return screenGui
end

